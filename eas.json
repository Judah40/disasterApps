{
  "cli": {
    "version": ">= 3.9.0"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "buildConfiguration": "Debug",
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "production": {
      "channel":"production",
      "env":{
        "EXPO_PUBLIC_API_URL":"https://drr.southafricanorth.cloudapp.azure.com/api/v1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
